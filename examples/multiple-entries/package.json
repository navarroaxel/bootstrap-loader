{
  "name": "bootstrap-loader-example",
  "version": "1.0.0",
  "description": "Example usage of bootstrap-loader with multiple entries with their own config",
  "main": "server.prod.js",
  "engines": {
    "node": "5"
  },
  "scripts": {
    "start": "nodemon --watch app/markup --ext html server.dev.js",
    "clean": "rm -rf public/",
    "lint": "eslint --ext .js,.jsx .",
    "build": "npm run clean && webpack --config webpack.prod.config.js",
    "prod": "nodemon --watch app/markup --ext html server.prod.js",
    "bs:prod": "npm run build && npm run prod",
    "install-local": "npm install --save-dev ../.."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shakacode/bootstrap-loader"
  },
  "keywords": [
    "bootstrap",
    "webpack"
  ],
  "authors": [
    "Axel Navarro <navarroaxel@gmail.com> (https://github.com/navarroaxel)"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.3",
    "bootstrap": "4.0.0-alpha.4",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.6.3",
    "jquery": "^2.1.4",
    "tether": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.20",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "body-parser": "^1.14.1",
    "bootstrap-loader": "*",
    "css-loader": "^0.22.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "font-awesome-loader": "^1.0.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.8.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.4",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
